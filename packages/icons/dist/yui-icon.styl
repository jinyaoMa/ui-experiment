@font-face
  font-family 'yui-icon'
  src url('./yui-icon.eot')
  src url('./yui-icon.eot?#iefix') format('embedded-opentype'), url('./yui-icon.woff') format('woff2'), url('./yui-icon.woff') format('woff'), url('./yui-icon.ttf') format('truetype'), url('./yui-icon.svg#yui-icon') format('svg')

.yui-icon
  font-family 'yui-icon'
  -moz-osx-font-smoothing grayscale
  -webkit-font-smoothing antialiased
  display inline-block
  font-style normal
  font-variant normal
  text-rendering auto
  line-height 1
  display inline-flex

.yui-icon-archive:before
  content '\78'

.yui-icon-bars:before
  content '\79'

.yui-icon-envelope:before
  content '\7a'

.yui-icon-folder-open:before
  content '\7b'

.yui-icon-home:before
  content '\7c'

.yui-icon-ma-jinyao:before
  content '\7d'

.yui-icon-search:before
  content '\7e'

.yui-icon-sitemap:before
  content '\7f'

.yui-icon-tag:before
  content '\80'

.yui-icon-tags:before
  content '\81'

.yui-icon-user:before
  content '\82'
